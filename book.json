{
    "title": "米米论坛",
    "author": "mimi",
    "description": "米米论坛,hebye搭建的 Gitbook 个人博客,详情请访问 https://hebye.github.io/",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    "links": {
	"sidebar": {"我的网站": "https://hebye.com/"
	}
	},	
    "plugins": [
	"github",
        "toc",
        "pageview-count",
        "mermaid-gb3",
        "-lunr",
        "-search",
        "search-plus",
        "splitter",
        "-sharing",
        "sharing-plus",
        "expandable-chapters-small",
        "anchor-navigation-ex",
        "edit-link",
        "copy-code-button",
        "chart",
        "favicon-custom",
        "github-buttons",
        "advanced-emoji",
        "tbfed-pagefooter",
	"donate",
	"tbfed-pagefooter",
	"mygitalk"
    ],
    "pluginsConfig": {
        "github": {
			"url": "https://github.com/hebye/hebye.github.io"
		},
        "favicon": "favicon.ico",
        "appleTouchIconPrecomposed152": "apple-touch-icon-precomposed-152.png",
        "toc": {
            "addClass": true,
            "className": "toc"
        },
        "github-buttons": {
            "buttons": [{
                "user": "hebye",
                "repo": "hebye.github.io",
                "type": "star",
                "size": "small"
            }]
        },
        "sharing": {
            "douban": true,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": true,
            "qzone": true,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": [
                "facebook", "google", "twitter",
                "weibo", "instapaper", "linkedin",
                "pocket", "stumbleupon"
            ]
        },
        "edit-link": {
            "base": "https://github.com/hebye/hebye.github.io/blob/master",
            "label": "编辑本页"
        },
        "chart": {
            "type": "c3"
        },
	 "donate": {
            "wechat": "/wechat.jpg",
            "alipay": "/alipay.jpg",
            "title": "",
            "button": "赏",
            "alipayText": "支付宝",
            "wechatText": "微信"
        },
	 "tbfed-pagefooter": {
            "copyright": "Copyright &copy hebye 2019",
            "modify_label": "文件修订时间: ",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
	 "mygitalk": {
            "clientID": "31081177a5fc6526a297",
            "clientSecret": "0d5d8e2719058599d9005ddcecdc7c238675738b",
            "repo": "GitHub repo",
            "owner": "GitHub repo ownwer",
            "admin": ["GitHub repo owner and collaborators, only these guys can initialize github issues"],
            "distractionFreeMode": false
        }
  }
}
